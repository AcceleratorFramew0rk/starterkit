      name: "WebIntranetSubnet"
      address_prefixes: ["{{ WebIntranetSubnet_address_prefixes }}"]
{% endif -%}
{% if WebSubnet_address_prefixes | trim != 'None' and WebSubnet_address_prefixes | trim != '' -%} 
# -----------------
